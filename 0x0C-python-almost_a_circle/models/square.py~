#!/usr/bin/python3
"""the class Square that inherits from Rectangle"""
from models.rectangle import Rectangle

class square(Rectangle):
    """the class Square that inherits from Rectangle"""
    def __init__(self, size, x=0, y=0, id=None):
        super().__init__(id)
        super().__init__(x)
        super().__init__(y)
        super().__init__(width)
        super().__init__(height)
