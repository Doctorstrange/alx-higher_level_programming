#include "main.h"
/**
* times_table - return 0 capital and small 1
* Return: Always 0 (Success)
*/
void times_table(void)
{
for (int i = 0; i < 10; i++)
{
for (int j = 0; j < 10; j++)
{
if (j == 0)
{
_putchar(i * j + '0');
}
else
{
_putchar('\t');
_putchar(i * j + '0');
}
}
_putchar('$');
_putchar('\n');
}
return 0;
}
